{
  "applicationId": "js.neutralino.zero",
  "version": "1.0.0",
  "defaultMode": "window",
  "documentRoot": "/my-app-svelte/dist/",
  "url": "/",
  "enableServer": true,
  "enableNativeAPI": true,
  "nativeAllowList": [
    "app.*",
    "os.*",
    "filesystem.*",
    "storage.*",
    "computer.*"
  ],
  "modes": {
    "window": {
      "title": "myapp",
      "width": 800,
      "height": 600,
      "minWidth": 400,
      "minHeight": 200,
      "icon": "/my-app-svelte/src/assets/svelte.png",
      "enableInspector": true
    }
  },
  "cli": {
    "binaryName": "myapp",
    "resourcesPath": "/my-app-svelte/dist/",
    "extensionsPath": "/extensions/",
    "clientLibrary": "/my-app-svelte/public/neutralino.js",
    "binaryVersion": "4.6.0",
    "clientVersion": "3.5.0",
    "frontendLibrary": {
      "patchFile": "/my-app-svelte/index.html",
      "devUrl": "http://localhost:3000"
    }
  }
}
